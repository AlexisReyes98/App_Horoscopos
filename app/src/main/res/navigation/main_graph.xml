<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/horoscopeFragment">

    <fragment
        android:id="@+id/horoscopeFragment"
        android:name="com.example.app_horoscopos.UI.Horoscope.HoroscopeFragment"
        android:label="fragment_horoscopo"
        tools:layout="@layout/fragment_horoscopo" >
        <action
            android:id="@+id/action_horoscopeFragment_to_horoscopeDetailActivity"
            app:destination="@id/horoscopeDetailActivity" />
    </fragment>
    <fragment
        android:id="@+id/luckFragment"
        android:name="com.example.app_horoscopos.UI.Luck.LuckFragment"
        android:label="fragment_luck"
        tools:layout="@layout/fragment_luck" />
    <fragment
        android:id="@+id/palmistryFragment"
        android:name="com.example.app_horoscopos.UI.Palmistry.PalmistryFragment"
        android:label="fragment_palmistry"
        tools:layout="@layout/fragment_palmistry" />
    <activity
        android:id="@+id/horoscopeDetailActivity"
        android:name="com.example.app_horoscopos.UI.Detail.HoroscopeDetailActivity"
        android:label="activity_horoscope_detail"
        tools:layout="@layout/activity_horoscope_detail" >
        <argument
            android:name="type"
            app:argType="com.example.app_horoscopos.DOMAIN.Model.HoroscopeModel" />
    </activity>
</navigation>